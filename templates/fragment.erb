Host <%= @title %>
  HostName <%= @remote_hostname %>
  Port <%= @remote_port %>
  User <%= @remote_username %>
  IdentityFile <%= @synthesized_privkey_path %>
  IdentitiesOnly yes
<% if @no_host_check -%>
  StrictHostKeyChecking no
<% end %>
<% if @connect_timeout -%>
  ConnectTimeout <%=  @connect_timeout %>
<% end %>
